{
  "name": "@alea/plugin-skeleton",
  "version": "0.1.0",
  "description": "Basic skeleton for plugin development",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "webpack:build": "webpack-cli -c webpack.config.js",
    "webpack:dev": "webpack-dev-server",
    "test": "npx jest"
  },
  "keywords": [
    "fastify",
    "plugin",
    "development"
  ],
  "author": "Henryk Daneluz",
  "license": "ISC",
  "main": "./dist/main.js",
