{
  "name": "@3Lowz/fastify-module-react-template",
  "version": "0.1.0",
  "description": "Template for fastify plugin with micro-FE UI development environment exposed through webpack module-federation",
  "main": "dist/index.js",
  "module": "./dist/module.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "publishConfig": {
    "@green23:registry": "https://gitlab.local.alea.pro/api/v4/projects/<<projectId>>/packages/npm/"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "webpack:build:prod": "webpack-cli -c we`bpack/webpack.production.config.js",
    "webpack:build": "webpack-cli -c webpack/webpack.development.config.js",
    "webpack:dev": "webpack-dev-server -c ./webpack/webpack.development.config.js",
    "test": "npx jest",
    "test:ci": "docker run --entrypoint bash --rm -w $PWD -v $PWD:$PWD -v $PWD/.env.testing:$PWD/.env.testing -v /var/run/docker.sock:/var/run/docker.sock gitlab/gitlab-runner:latest -c 'git config --global --add safe.directory \"*\";gitlab-runner exec docker test_job'",
    "initdb": "npx mikro-orm schema:fresh --run"
  },
  "keywords": [
    "fastify",
    "plugin",
    "development"
  ],
  "author": "Henryk Daneluz",
  "license": "ISC",
  "dependencies": {
    "fastify-plugin": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router": "^6.4.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^6.4.2",
    "reactstrap": "^9.1.4",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3",
    "simplebar-react": "^3.2.4"
  },
  "peerDependencies": {
    "@mikro-orm/core": "^6.3.1",
    "@mikro-orm/mysql": "^6.3.1"
  },
  "federation": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.4.2",
    "reactstrap": "9.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@fastify/swagger": "^8.8.0",
    "@mikro-orm/cli": "^6.3.1",
    "@mikro-orm/migrations": "^6.3.1",
    "@mikro-orm/seeder": "^6.3.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.8.4",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/react-redux": "^7.1.25",
    "@types/reactstrap": "^8.7.2",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "fastify": "^4.17.0",
    "fastify-tsconfig": "^1.0.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.6.2",
    "jsdocs": "^0.0.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.8.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.15.1"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./test/mock/mikro-orm.config.ts"
    ]
  }
}
